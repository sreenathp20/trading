{
    "version": "0.2.0",
    "configurations": [
    {
        "name": "Attach to Chrome",
        "port": 4200,
        "request": "launch",
        "type": "chrome",
        "webRoot": "${workspaceFolder}"
    },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/app/backend/trading/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Python: File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "justMyCode": true
        }
    ]
}